#  Calling the dynamic library.
useDynLib(edgeR, .registration=TRUE, .fixes=".cxx_")

#  All functions exported other than those starting with "."
exportPattern("^[^\\.]")

exportClasses("DGEList","DGEExact","DGEGLM","DGELRT","TopTags")
exportMethods("show")

